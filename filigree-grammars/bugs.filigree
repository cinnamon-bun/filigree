smallNoun = [<fruit>/<veg>/<flower>/<otherFlavor>]
bigNoun = [rock/garden/creek/flower/bush/bramble/mushroom/log/meadow/tree]
noun = [<smallNoun>/<bigNoun>]

size = [big/little/small/lil/tiny]
